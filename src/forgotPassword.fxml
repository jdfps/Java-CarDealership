<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="457.0" prefWidth="436.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ForgotPassword">
   <children>
      <TextField fx:id="FirstName_Field" layoutX="100.0" layoutY="82.0" prefHeight="26.0" prefWidth="319.0" promptText="Verify First Name" />
      <TextField fx:id="LastName_Field" layoutX="100.0" layoutY="128.0" prefHeight="26.0" prefWidth="319.0" promptText="Verify Last Name" />
      <TextField fx:id="Email_Field" layoutX="100.0" layoutY="170.0" prefHeight="26.0" prefWidth="319.0" promptText="Verify Email" />
      <TextField fx:id="Phone_Field" layoutX="100.0" layoutY="214.0" prefHeight="26.0" prefWidth="319.0" promptText="Veryify Phone Number  (no spaces)" />
      <Label layoutX="24.0" layoutY="86.0" text="First Name: " />
      <Label layoutX="24.0" layoutY="132.0" text="Last Name: " />
      <Label layoutX="21.0" layoutY="175.0" text="Verify Email: " />
      <Label layoutX="8.0" layoutY="219.0" text="Verify Phone #: " />
      <Button layoutX="100.0" layoutY="267.0" mnemonicParsing="false" onAction="#_enter" prefHeight="26.0" prefWidth="112.0" text="Enter" />
      <Button layoutX="260.0" layoutY="267.0" mnemonicParsing="false" onAction="#_clear" prefHeight="26.0" prefWidth="112.0" text="Clear" />
      <Label fx:id="show_password" alignment="TOP_CENTER" layoutX="37.0" layoutY="359.0" prefHeight="23.0" prefWidth="362.0" underline="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#_back" text="Back" />
   </children>
</AnchorPane>
